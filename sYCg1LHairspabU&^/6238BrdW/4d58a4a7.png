    <script>
        // config.js
        const CONFIG = {
            // Bounds optimized for Delhi, Gurugram, and Noida
            BOUNDS: {
                north: 28.8,  // Northern Delhi
                south: 28.4,  // Southern Gurugram
                east: 77.6,   // Eastern Noida
                west: 76.8    // Western Gurugram
            },

            // Restricted to only Delhi, Gurugram, and Noida with their variations
            NCR_REGIONS: {
                'Delhi': [
                    'delhi',
                    'new delhi',
                    'south delhi',
                    'north delhi',
                    'east delhi',
                    'west delhi',
                    'central delhi',
                    'delhi ncr'
                ],
                'Gurugram': [
                    'gurugram',
                    'gurgaon',
                    'cyber city',
                    'cyber hub',
                    'udyog vihar',
                    'dlf cyber city',
                    'golf course road',
                    'sohna road'
                ],
                'Noida': [
                    'noida',
                    'greater noida',
                    'noida extension',
                    'sector'  // Many Noida locations start with 'Sector'
                ]
            },

            MESSAGES: {
                INVALID_CITY: 'Please select a location only in Delhi, Gurugram, or Noida',
                NO_PLACE_SELECTED: 'Please select a valid location from the dropdown',
                INVALID_ADDRESS: 'Invalid address. Please provide a valid address within Delhi, Gurugram, or Noida'
            }
        };

        // app.js
        let autocomplete;

        function initPlacesAutocomplete() {
            const input = document.getElementById('pincode-input');

            // Create bias bounds
            const defaultBounds = new google.maps.LatLngBounds(
                { lat: CONFIG.BOUNDS.south, lng: CONFIG.BOUNDS.west },
                { lat: CONFIG.BOUNDS.north, lng: CONFIG.BOUNDS.east }
            );

            // Configure the autocomplete options
            const options = {
                bounds: defaultBounds,
                componentRestrictions: { country: 'IN' },
                fields: ['address_components', 'geometry', 'name', 'formatted_address'],
                strictBounds: true  // Changed to true to restrict to our bounds
            };

            // Create the autocomplete object
            autocomplete = new google.maps.places.Autocomplete(input, options);

            // Add event listeners
            autocomplete.addListener('place_changed', handlePlaceSelection);
            input.addEventListener('input', handleInput);
        }

        function handleInput() {
            hideResultContainer();
            removeErrorMessage();
        }

        function handlePlaceSelection() {
            const place = autocomplete.getPlace();
            const input = document.getElementById('pincode-input');

            if (!place.geometry) {
                showErrorMessage(CONFIG.MESSAGES.NO_PLACE_SELECTED);
                return;
            }

            // Mark the address as selected from autocomplete
            document.getElementById('is_autocomplete').value = '1';

            // Check if location is in allowed regions
            const locationCheck = isLocationAllowed(place);

            if (!locationCheck.allowed) {
                showErrorMessage(CONFIG.MESSAGES.INVALID_CITY);
                input.value = '';
                hideResultContainer();
                return;
            }

            selectedPlace = {
                name: place.name,
                address: place.formatted_address,
                region: locationCheck.region,
                location: {
                    lat: place.geometry.location.lat(),
                    lng: place.geometry.location.lng()
                }
            };

            displaySelectedPlace();
        }

        function isLocationAllowed(place) {
            const result = { allowed: false, region: '' };

            // Get all relevant parts of the address
            const addressParts = {
                locality: '',
                sublocality: '',
                administrative_area: ''
            };

            // Extract address components
            for (const component of place.address_components) {
                if (component.types.includes('locality')) {
                    addressParts.locality = component.long_name.toLowerCase();
                }
                if (component.types.includes('sublocality_level_1')) {
                    addressParts.sublocality = component.long_name.toLowerCase();
                }
                if (component.types.includes('administrative_area_level_2')) {
                    addressParts.administrative_area = component.long_name.toLowerCase();
                }
            }

            // Check if the location matches any of our allowed regions
            for (const [region, variations] of Object.entries(CONFIG.NCR_REGIONS)) {
                // Check each variation against all address parts
                const isMatch = variations.some(variation => {
                    return Object.values(addressParts).some(part => {
                        if (variation === 'sector' && region === 'Noida') {
                            // Special handling for Noida sectors
                            return part.includes('sector') &&
                                (part.includes('noida') || addressParts.administrative_area.includes('noida'));
                        }
                        return part.includes(variation);
                    });
                });

                if (isMatch) {
                    result.allowed = true;
                    result.region = region;
                    break;
                }
            }

            // If no match found by name, check coordinates as fallback
            if (!result.allowed) {
                const lat = place.geometry.location.lat();
                const lng = place.geometry.location.lng();

                // Only allow if within our specified bounds
                if (lat >= CONFIG.BOUNDS.south &&
                    lat <= CONFIG.BOUNDS.north &&
                    lng >= CONFIG.BOUNDS.west &&
                    lng <= CONFIG.BOUNDS.east) {

                    result.allowed = true;
                    result.region = determineRegionByCoordinates(lat, lng);
                }
            }

            return result;
        }

        function determineRegionByCoordinates(lat, lng) {
            // More precise region determination based on coordinates
            if (lng < 77.1) return 'Gurugram';    // Western region (Gurugram)
            if (lng > 77.35) return 'Noida';      // Eastern region (Noida)
            return 'Delhi';                        // Central region (Delhi)
        }

        // Handle manual input (if the user types an address instead of selecting it)
        document.getElementById('pincode-input').addEventListener('blur', function () {
            const isAutocomplete = document.getElementById('is_autocomplete').value === '1';

            // If the address was typed manually and is not from autocomplete
            if (!isAutocomplete) {
                handleManualInput(this);
            }
        });

        function handleManualInput(input) {
            // When the user types an address manually, ensure it is in the allowed regions
            const place = { formatted_address: input.value };

            const locationCheck = isLocationAllowed(place);

            if (!locationCheck.allowed) {
                showErrorMessage(CONFIG.MESSAGES.INVALID_ADDRESS);
                input.value = '';  // Clear the input if invalid
            }
        }

        function showErrorMessage(message) {
            // Function to display an error message to the user
            alert(message);
            // Optionally, you can add additional UI elements to show the message
        }

        function hideResultContainer() {
            // Hide the results container if needed
        }

        function removeErrorMessage() {
            // Remove any error message if needed
        }

    </script>

