        <script>


            function removeItem(key, service_id = 0) {
                // AJAX request to remove the item from the session cart
                var xhr = new XMLHttpRequest();
                xhr.open('POST', 'remove_from_cart.php?service_id=' + service_id, true);
                xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
                xhr.onreadystatechange = function () {
                    if (xhr.readyState == XMLHttpRequest.DONE) {
                        if (xhr.status == 200) {
                            // Success: reload the page or update the cart display
                            window.location.reload(); // Reload the page for simplicity
                        } else {
                            // Error: Handle the error response
                            console.error('Error removing item from cart:', xhr.responseText);
                        }
                    }
                };
                xhr.send('key=' + key);
            }
        </script>

